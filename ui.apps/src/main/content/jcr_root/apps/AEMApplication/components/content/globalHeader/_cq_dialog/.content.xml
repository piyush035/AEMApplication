<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="GlobalHeader Component Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <links
                jcr:primaryType="nt:unstructured"
                jcr:title="Header Components"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <logoHomePageUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Enter Logo Section Landing Page Url."
                                fieldLabel="Logo Section Landing Page Url"
                                name="./headerlogohomepageurl"
                                rootPath="/content"/>
                            <homePage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Enter the  Section Landing page url."
                                fieldLabel="Section Landing Page Url"
                                name="./headerhomepageurl"
                                rootPath="/content"/>
							<itemLinks
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/apps/AEMApplication/components/content/multifield"
                                fieldLabel="Add Component">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    name="./commonheaderlinks">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                            	<levelOneLabel
													jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Menu Label"
                                                    fieldLabel="Menu Label"
                                                    name="./menuLabel"/>
                                                <link
					                                jcr:primaryType="nt:unstructured"
					                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
					                                fieldDescription="Enter the page url."
					                                fieldLabel="Enter the page url"
					                                name="./link"
					                                rootPath="/link"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </itemLinks>
                        </items>
                    </columns>
                </items>
            </links>
        </items>
    </content>
</jcr:root>
